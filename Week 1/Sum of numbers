#include <iostream>

using namespace std;

int main()
{
    int x , y;
    cin>>x >> y;
    int z = x + y;
    cout << z;
    return 0;
}


----------------------------------------------------------------------------------------


#include <iostream>

using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t != 0){
        int x , y;
        cin>>x >> y;
        int z = x + y;
        cout << z;
        t = t - 1;
    }
    return 0;
}
